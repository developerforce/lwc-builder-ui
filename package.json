{
  "name": "lwc-builder",
  "description": "A tool to generate Lightning Web Component (LWC) files",
  "version": "0.1.0",
  "author": "ninoish",
  "bugs": "https://github.com/developerforce/lwc-builder/issues",
  "dependencies": {
    "@salesforce-ux/design-system": "^2.14.2",
    "change-case": "^4.1.2",
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "file-saver": "^2.0.5",
    "helmet": "^4.4.1",
    "jszip": "^3.5.0"
  },
  "devDependencies": {
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-jsdoc": "^31.6.1",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^4.3.8",
    "lwc-services": "^3.0.2",
    "prettier": "^2.2.1"
  },
  "engines": {
    "node": ">=10.13.0",
    "npm": ">=6.4.1"
  },
  "homepage": "https://github.com/developerforce/lwc-builder",
  "husky": {
    "hooks": {
      "pre-push": "npm run lint && npm run prettier"
    }
  },
  "keywords": [
    "lwc"
  ],
  "license": "BSD-3-Clause",
  "repository": "https://github.com/developerforce/lwc-builder",
  "scripts": {
    "build": "lwc-services build -m production",
    "build:development": "lwc-services build",
    "lint": "eslint src --ext js",
    "lint:fix": "eslint src --ext js --fix",
    "prettier": "prettier --write \"**/*.{css,html,js,json,md,ts,yaml,yml}\"",
    "prettier:verify": "prettier --list-different \"**/*.{css,html,js,json,md,ts,yaml,yml}\"",
    "serve": "node scripts/server.js",
    "test:unit": "lwc-services test:unit",
    "test:unit:coverage": "lwc-services test:unit --coverage",
    "test:unit:debug": "lwc-services test:unit --debug",
    "test:unit:watch": "lwc-services test:unit --watch",
    "watch": "lwc-services watch"
  }
}
